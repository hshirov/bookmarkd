{
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./src",
    "paths": {
      "src/*": ["./*"],
      "api/*": ["./api/*"],
      "components/*": ["./components/*"],
      "enums/*": ["./enums/*"],
      "hooks/*": ["./hooks/*"],
      "interfaces/*": ["./interfaces/*"],
      "screens/*": ["./screens/*"],
      "store/*": ["./store/*"],
      "styles/*": ["./styles/*"],
      "types/*": ["./types/*"],
      "utils/*": ["./utils/*"]
    }
  },
  "typeRoots": ["./src/typings", "./node_modules/@types"],
  "extends": "expo/tsconfig.base"
}
