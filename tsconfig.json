{
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "src/*": ["./src/*"],
      "api/*": ["./src/api/*"],
      "components/*": ["./src/components/*"],
      "enums/*": ["./src/enums/*"],
      "hooks/*": ["./src/hooks/*"],
      "screens/*": ["./src/screens/*"],
      "styles/*": ["./src/styles/*"],
      "types/*": ["./src/types/*"]
    }
  },
  "typeRoots": ["./src/typings", "./node_modules/@types"],
  "extends": "expo/tsconfig.base"
}
